moothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  height: 900px;\n\n  background-image: url(\"./assets/sentry-pattern.png\");\n  background-position: center;\n  background-repeat: no-repeat;\n  background-size: cover;\n}\n\n#email-div {\n  width: 600px;\n  display: inline-flex;\n}\n\n#emailInput {\n  height: 30px;\n  width: 90%;\n}\n\n#event-list {\n  height: 400px;\n  padding: 3rem;\n}\n\n#greeting {\n  margin-bottom: 0.8rem;\n  margin-top: -2rem;\n  font-size: 2rem;\n}\n\n.icon {\n  height: 6rem;\n  width: 6rem;\n  padding-top: 1.6rem;\n  padding-left: 2rem;\n}\n\n#sentryIcon {\n  height: 9rem;\n  width: 9rem;\n}\n\n#iconsDiv {\n  display: inline-flex;\n  padding-bottom: 2rem;\n}\n\n.plus {\n  font-size: 2.6rem;\n}\n</style>\n"]}]}